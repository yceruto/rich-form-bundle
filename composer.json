{
    "name": "yceruto/rich-form-bundle",
    "type": "symfony-bundle",
    "description": "Generous form features for Symfony web applications",
    "keywords": ["form"],
    "homepage": "https://github.com/yceruto/RichFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto Gonzalez",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/form": "~3.4|~4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\Bundle\\RichFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTestBundle\\": "tests/Fixtures/AppTestBundle/",
            "EasyCorp\\Bundle\\EasyAdminBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
