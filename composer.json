{
    "name": "yceruto/rich-form-bundle",
    "type": "symfony-bundle",
    "description": "Form features for Symfony applications",
    "keywords": ["form"],
    "homepage": "https://github.com/yceruto/RichFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto Gonzalez",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/doctrine-bridge": "^5.1",
        "symfony/form": "^5.1.3",
        "symfony/framework-bundle": "^5.1",
        "symfony/options-resolver": "^5.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "doctrine/orm": "^2.7",
        "symfony/asset": "^5.1",
        "symfony/panther": "^0.7",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/translation-contracts": "^1.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\Bundle\\RichFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Fixtures/App/AppKernel.php"
        ],
        "psr-4": {
            "App\\Test\\": "tests/Fixtures/App/src/",
            "Yceruto\\Bundle\\RichFormBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
