{
    "name": "yceruto/rich-form-bundle",
    "type": "symfony-bundle",
    "description": "Generous form features for Symfony web applications",
    "keywords": ["form"],
    "homepage": "https://github.com/yceruto/RichFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto Gonzalez",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6",
        "symfony/doctrine-bridge": "~3.4|~4.0",
        "symfony/form": "~3.4|~4.0",
        "symfony/framework-bundle": "~3.4|~4.0",
        "symfony/options-resolver": "^4.2"
    },
    "require-dev": {
        "symfony/panther": "^0.2.0",
        "symfony/phpunit-bridge": "~3.4|~4.0",
        "symfony/var-dumper": "~3.4|~4.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\Bundle\\RichFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yceruto\\Bundle\\RichFormBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
