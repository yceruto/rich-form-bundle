{
    "name": "yceruto/rich-form-bundle",
    "type": "symfony-bundle",
    "description": "Form features for Symfony applications",
    "keywords": ["form"],
    "homepage": "https://github.com/yceruto/RichFormBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yonel Ceruto Gonzalez",
            "email": "yonelceruto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "symfony/doctrine-bridge": "^3.4|^4.2",
        "symfony/form": "^3.4|^4.2",
        "symfony/framework-bundle": "^3.4|^4.2",
        "symfony/options-resolver": "^4.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "doctrine/orm": "^2.6",
        "symfony/asset": "^3.4|^4.2",
        "symfony/panther": "^0.4",
        "symfony/phpunit-bridge": "^3.4|^4.2",
        "symfony/twig-bundle":"^3.4|^4.2",
        "symfony/var-dumper": "^3.4|^4.2",
        "symfony/yaml": "^3.4|^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Yceruto\\Bundle\\RichFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Fixtures/App/AppKernel.php"
        ],
        "psr-4": {
            "App\\Test\\": "tests/Fixtures/App/src/",
            "Yceruto\\Bundle\\RichFormBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
